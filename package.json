{
  "name": "requirebin",
  "version": "0.1.1",
  "description": "create programs in the browser using modules from NPM",
  "scripts": {
    "start": "beefy index.js:js/bundle.js 8080",
    "build": "browserify index.js -o js/bundle.js --debug",
    "minify": "cat js/bundle.js | uglifyjs -o js/bundle.js"
  },
  "repository": "git://github.com/intesso/requirebin",
  "dependencies": {
    "browser-module-sandbox": "~1.3.3",
    "browser-request": "~0.3.1",
    "concat-stream": "1.0.0",
    "detective": "~2.1.2",
    "element-class": "0.0.2",
    "javascript-editor": "~0.2.1",
    "keydown": "~0.0.3",
    "md5-jkmyers": "~0.0.1",
    "source-map": "^0.2.0",
    "nets": "^1.0.0"
  },
  "engines": {
    "node": ">=0.10.x"
  },
  "devDependencies": {
    "browserify": "~2.23.1",
    "beefy": "~0.4.0",
    "uglify-js": "~2.4.12"
  }
}
